<project>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>1.8</source>
                    <target>1.8</target>
                </configuration>
            </plugin>
        </plugins>
    </build>
    <modelVersion>4.0.0</modelVersion> 
    <groupId>utp</groupId> 
    <artifactId>ProyectoJavaMaven</artifactId> 
    <version>1.0</version> 
    <packaging>jar</packaging> 
    <properties> 
        <sonar.projectKey>Susanelizabetth_ProyectoJavaMaven</sonar.projectKey>
        <sonar.organization>susanelizabetth</sonar.organization>
        <sonar.host.url>https://sonarcloud.io</sonar.host.url>
    </properties> 
</project> 